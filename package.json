    "fonts",
    "signed",
    "distance",
    "field",
    "glyph",
    "glyphs",
    "sdf",
    "msdf",
    "multichannel"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Apidcloud/msdf-bmfont-xml.git"
  },
